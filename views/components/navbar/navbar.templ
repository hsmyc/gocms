package navbar

import "hsmyc/gocms/models"

templ Navbar(menu []models.NavbarItem) {
	<div class="navbar">
		<div>
			<a href="/">
				<div>
					<i class="fa-solid fa-house"></i>
					GoCMS
				</div>
			</a>
		</div>
		<div class="navbar_items">
			for _, item := range menu {
				<div class="navbar_item">
					<a href={ item.Url }>{ item.Title }</a>
				</div>
			}
		</div>
	</div>
}
