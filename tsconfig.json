{
  "compilerOptions": {
    "outDir": "./static/scripts/compiled",
    "rootDir": "./static/scripts/raw",
    "noImplicitAny": true,
    "module": "ES2022",
    "target": "es6",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noEmit": true
  },
  "include": ["static/scripts/raw/**/*.ts"],
  "exclude": ["./static/scripts/compiled"]
}
